{
    "alias": "masquerade.phar",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Json",
        "KevinGH\\Box\\Compactor\\Php"
    ],
    "compression": "GZ",
    "directories": [
        "src"
    ],
    "files": [
        "README.md",
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "Tests",
                "doctrine/cache",
                "doctrine/annotations",
                "nesbot"
            ],
            "in": "vendor"
        }
    ],
    "output": "dist/masquerade.phar"
}
